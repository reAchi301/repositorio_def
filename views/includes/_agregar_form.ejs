<form action="/plantas/agregar" method="POST">
  <form action="/plantas/agregar" method="POST" enctype="multipart/form-data">
      <input type="hidden" name="_csrf" value="<%= csrfToken %>" >
      <label for="nombre" class="label">Nombre de la planta</label>
      <div class="select" id="nombre">
        <select name="id_planta">
          <% for(let planta of plantas) { %>
            <option value="<%= planta.id %>"><%= planta.nombre %></option>
          <% } %>
        </select>
      </div>
      <br><br>
      <div class="file">
        <label for="imagen" class="file-label">
          <input type="file" class="file-input" id="imagen" name="imagen">
          <span class="file-cta">
            <span class="file-label"> Escoge una imagenâ€¦ </span>
          </span>
        </label>
      </div>
      <input class="button is-info" type="submit" value="Enviar">
  </form>